int compare(const void *a, const void *b) {
    return (*(int *)a - *(int *)b);
}

bool containsDuplicate(int *nums, int numsSize) {
    // Sort the array. If there are duplicates, they will be adjacent after sorting.
    qsort(nums, numsSize, sizeof(int), compare);

    // Iterate through the sorted array and check for adjacent duplicates.
    for (int i = 0; i < numsSize - 1; i++) {
        if (nums[i] == nums[i + 1]) {
            return true; // Found a duplicate
        }
